<p id="notice"><%= notice %></p>

<h1>Tables</h1>

<table>
  <thead>
    <tr>
      <th>体重</th>
      <th>BMI</th>
      <th>体脂肪</th>
      <th>水分</th>
      <th>筋肉量</th>
      <th>骨量</th>
      <th>基礎代謝</th>
      <th>内臓脂肪</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tables.each do |table| %>
      <tr>
        <td><%= table.weight %></td>
        <td><%= table.bmi %></td>
        <td><%= table.fat %></td>
        <td><%= table.moisture %></td>
        <td><%= table.muscle %></td>
        <td><%= table.bone %></td>
        <td><%= table.metabolism %></td>
        <td><%= table.visceral_fat %></td>
        <td><%= link_to 'Show', table %></td>
        <td><%= link_to 'Edit', edit_table_path(table) %></td>
        <td><%= link_to 'Destroy', table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Table', new_table_path %>
