<h1>Log in</h1>
<%=form_for(:session,url:login_path) do |form| %>
<div class="field">
  <%= form.label:email %>
  <%= form.email_field :email %>
</div>

<div class="field">
  <%= form.label:password %>
  <%= form.password_field :password %>
</div>

<div class="field">
  <%= form.submit"Log in" %>
</div>

新規登録の方は<%= link_to "サインイン", new_user_path %>
<% end %>
